@model IEnumerable<Eagle.Services.Dtos.SystemManagement.Identity.UserContactDetail>

<div class="box box-danger">
    <div class="box-header with-border">
        <div class="box-tools pull-right">
            <span class="label label-danger">@Model.Count() @LanguageResource.MemberList</span>
        </div>
        <h4 class="box-title">@LanguageResource.LatestMembers</h4>
    </div>
    <!-- /.box-header -->
    <div class="box-body">
        <ul class="member-group">
            @if (Model != null && Model.Any())
            {
                foreach (var item in Model)
                {
                    <li>
                        <div id="@item.UserId" class="member">
                            <img src="@Url.Content(item.Profile.Contact.FileUrl)" class="img-responsive" alt="@item.Profile.Contact.DocumentInfo.FileTitle">
                            <a class="users-list-name list-item" href="#">
                                @item.Profile.Contact.FullName
                            </a>
                        </div>
                    </li>
                }
            }
            else
            {
                <li>
                    <div colspan="5" class="not-found col-xs-12 col-sm-12 col-md-12">@LanguageResource.NoDataFound</div>
                </li>
            }
        </ul>
    </div>
    <!-- /.box-body -->
    <div class="box-footer text-center">
        <a href="@Url.Action("Index","User")" class="uppercase">View All Users</a>
    </div>
    <!-- /.box-footer -->
</div>
