<div class="panel panel-default search-panel">
    <div class="panel-heading">
        <div class='page-header margin-top-5 margin-bottom-0'>
            <div class='pull-right'>
                <select id="SearchTypeId" name="SearchTypeId" data-url='@Url.Action("PoplulateMediaTypeSelectList", "MediaTopic")' class="form-control"></select>
            </div>
            <h4><span class="glyphicon glyphicon-star"></span> @LanguageResource.TreeList</h4>
        </div>
    </div>
    <div class="panel-body">
        <div class="container-fluid">
            <ul id="tree"></ul>
            @Html.Hidden("SelectTree")
        </div>
    </div>
    <div class="panel-footer"></div>
</div>