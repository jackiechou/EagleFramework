<div id="dual-list-box" class="form-group row dual-list-box module-container">
    <div class="col-md-5">
        <h4><span class="unselected-title">@LanguageResource.AvailableItems</span></h4>
        <input class="filter form-control filter-unselected" type="text" placeholder="Filter" style="margin-bottom: 5px;">
        @*<select id="cbxAvailableModule" name="cbxAvailableModule" class="unselected form-control" style="height: 200px; width: 100%;" multiple="multiple"></select>*@
        @Html.ListBox("AvailableModules", (MultiSelectList)ViewBag.AvailableModules, new { multiple = "multiple", @class = "select-from unselected form-control", size = 10 })
    </div>
    <div class="dual-list-buttons col-md-2 center-block" style="margin-top: 80px">
        <button type="button" class="btn btn-default col-md-8 col-md-offset-2 atr push-all" data-type="atr" style="margin-bottom: 10px;">
            <span class="glyphicon glyphicon-list"></span> <span class="glyphicon glyphicon-chevron-right"></span>
        </button>
        <button type="button" class="btn btn-default col-md-8 col-md-offset-2 str push-item" data-type="str" style="margin-bottom: 20px;">
            <span class="glyphicon glyphicon-chevron-right"></span>
        </button>
        <button type="button" class="btn btn-default col-md-8 col-md-offset-2 stl pull-item" data-type="stl" style="margin-bottom: 10px;">
            <span class="glyphicon glyphicon-chevron-left"></span>
        </button>
        <button type="button" class="btn btn-default col-md-8 col-md-offset-2 atl pull-all" data-type="atl" style="margin-bottom: 10px;">
            <span class="glyphicon glyphicon-chevron-left"></span> <span class="glyphicon glyphicon-list"></span>
        </button>
    </div>
    <div class="col-md-5">
        <h4><span class="selected-title">@LanguageResource.SelectedItems</span></h4>
        <input class="input-to filter form-control filter-selected" type="text" placeholder="Filter" style="margin-bottom: 5px;">
        @*<select id="cbxSelectedModule" name="cbxSelectedModule" class="selected form-control" style="height: 200px; width: 100%;" multiple="multiple"></select>*@
        @Html.ListBox("SelectedModules", (MultiSelectList)ViewBag.SelectedModules, new { multiple = "multiple", @class = "select-to selected form-control", size = 10 })
    </div>
</div>